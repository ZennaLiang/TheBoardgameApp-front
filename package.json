{
  "name": "bgapp-front",
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js"
    ],
    "coverageReporters": [
      "text"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/assetsTransformer.js",
      "\\.(css|less)$": "<rootDir>/assetsTransformer.js"
    }
  },
  "version": "0.1.0",
  "private": true,
  "homepage": "https://tselouie.github.io/TheBoardgameApp-front",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@mui/lab": "^7.0.0-beta.14",
    "@mui/material": "^7.2.0",
    "axios": "^1.10.0",
    "bootstrap": "^5.3.7",
    "cors-anywhere": "^0.4.4",
    "date-fns": "^4.1.0",
    "formik": "^2.4.6",
    "google-auth-library": "^10.1.0",
    "gsap": "^3.13.0",
    "in-view": "^0.6.1",
    "jquery": "^3.7.1",
    "match-sorter": "^8.0.3",
    "moment": "^2.30.1",
    "react": "^18.0.0",
    "react-big-calendar": "^1.19.4",
    "react-bootstrap": "^2.10.10",
    "react-datepicker": "^8.4.0",
    "react-dom": "^18.0.0",
    "react-loader-spinner": "^6.1.6",
    "react-modal": "^3.16.3",
    "react-router-dom": "^7.6.3",
    "react-scripts": "^5.0.1",
    "react-select": "^5.10.1",
    "react-table": "^7.8.0",
    "reactstrap": "^9.2.3",
    "sass": "^1.89.2",
    "socket.io-client": "^4.8.1",
    "xml2js": "^0.6.2",
    "yup": "^1.6.1"
